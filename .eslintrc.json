{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "no-console": 1,
    "no-unused-vars": "error",
    "no-duplicate-imports": "error",
    "no-warning-comments": [
      "warn",
      {
        "terms": ["todo", "fixme", "TODO", "FIXME", "FIX"],
        "location": "anywhere"
      }
    ]
  }
}
